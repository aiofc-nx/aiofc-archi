{
  "name": "@aiofc/source",
  "version": "0.0.0",
  "license": "MIT",
  "packageManager": "pnpm@8.15.0",
  "volta": {
    "node": "20.18.1"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "commit": "cz",
    "clean": "gulp clean:bundle",
    "format": "prettier --write \"**/src/**/*.ts\"",
    "lint": "eslint \"apps/**/*.ts\" \"packages/**/*.ts\" --fix",
    "doc": "typedoc --options typedoc.json && vitepress dev docs",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs:typedoc": "typedoc --options typedoc.json",
    "docs:typedoc:watch": "typedoc --watch"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "cz-customizable.config.js"
    }
  },
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@commitlint/types": "^19.5.0",
    "@eslint/js": "^9.8.0",
    "@nx/devkit": "20.5.0-rc.0",
    "@nx/eslint": "20.5.0-rc.0",
    "@nx/eslint-plugin": "20.5.0-rc.0",
    "@nx/jest": "20.5.0-rc.0",
    "@nx/js": "20.5.0-rc.0",
    "@nx/node": "20.5.0-rc.0",
    "@nx/plugin": "20.5.0-rc.0",
    "@nx/workspace": "20.5.0-rc.0",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.3.12",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@swc/jest": "~0.2.36",
    "@types/jest": "^29.5.12",
    "@types/node": "18.16.9",
    "commitizen": "^4.3.1",
    "cz-customizable": "^7.4.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "nx": "20.5.0-rc.0",
    "prettier": "^3.5.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typedoc": "^0.27.9",
    "typedoc-plugin-markdown": "^4.4.2",
    "typedoc-vitepress-theme": "^1.1.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.19.0",
    "verdaccio": "^6.0.5",
    "vitepress": "^1.6.3"
  },
  "dependencies": {
    "@aiofc/coding": "workspace:*",
    "@nx/devkit": "20.5.0-rc.0"
  }
}